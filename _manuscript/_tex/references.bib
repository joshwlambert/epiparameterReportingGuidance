	pages = {e0257978},
	file = {Full Text PDF:C\:\\Users\\gc4018\\Zotero\\storage\\N7V8AWFD\\Ward and Johnsen - 2021 - Understanding an evolving pandemic An analysis of.pdf:application/pdf},
}

