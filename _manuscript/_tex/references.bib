	journal = {International Journal of Infectious Diseases},
	author = {Nishiura, Hiroshi and Linton, Natalie M. and Akhmetzhanov, Andrei R.},
	month = apr,
	year = {2020},
	keywords = {Coronavirus, Epidemiology, Generation time, Illness onset, Outbreak, Statistical model, Viruses},
	pages = {284--286},
	file = {Full Text:C\:\\Users\\gc4018\\Zotero\\storage\\Y83XX3LW\\Nishiura et al. - 2020 - Serial interval of novel coronavirus (COVID-19) in.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\gc4018\\Zotero\\storage\\ITH3HB7N\\S1201971220301193.html:text/html},
}

